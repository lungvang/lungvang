{
  "name": "lungvang",
  "version": "1.0.0",
  "description": "Lung Vang Landing Page",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf node_modules/ && rm -rf public/ && rm -rf .cache/ && rm -rf package-lock.json && rm -rf yarn.lock",
    "build": "gatsby build",
    "dev": "gatsby develop",
    "start": "npm run dev",
    "deploy": "gatsby build && gh-pages -b master -d public",
    "check-staged": "lint-staged",
    "lint": "eslint src -c .eslintrc --ext js,jsx",
    "format": "prettier --write 'src/**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://thuanqh@github.com/lungvang/lungvang.github.io.git"
  },
  "keywords": [
    "lungvang"
  ],
  "author": "Jason Quach <thuanqh@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lungvang/lungvang.github.io/issues"
  },
  "homepage": "https://github.com/lungvang/lungvang.github.io#readme",
  "dependencies": {
    "gatsby": "2.8.2",
    "gatsby-plugin-manifest": "2.1.1",
    "gatsby-plugin-offline": "2.1.1",
    "gatsby-plugin-react-helmet": "3.0.12",
    "gatsby-plugin-styled-components": "3.0.7",
    "polished": "^3.4.0",
    "prop-types": "^15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1",
    "rebass": "^3.1.1",
    "styled-components": "4.2.1",
    "styled-icons": "^8.0.0",
    "styled-system": "^5.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-plugin-styled-components": "1.10.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "gh-pages": "2.0.1",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.7",
    "prettier": "1.17.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint"
    ],
    "*.{js,jsx,json,md}": [
      "prettier --list-different"
    ]
  }
}
